{
  "name": "n8n-nodes-microsoft-fabric",
  "version": "1.0.0",
	"description": "Custom SQL Server node compatible with Microsoft Fabric.",
  "license": "MIT",
  "homepage": "",
  "keywords": [
    "n8n-community-node-package",
    "fabric",
    "microsoft-fabric",
    "sql"
  ],
  "author": { "name": "Luan Marques", "email": "n8n@luanmarques.com.br" },
  "repository": {
    "type": "git",
    "url": "https://github.com/Luan7805/n8n-nodes-microsoft-fabric.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "npm run build"
  },
  "files": ["dist"],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": false,
    "credentials": [
      "dist/credentials/MicrosoftSql.credentials.ts",
      "dist/credentials/MicrosoftSqlServicePrincipal.credentials.ts"
    ],
    "nodes": ["dist/nodes/sql/MicrosoftSql.node.json"]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/mssql": "^9.1.9",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": { "n8n-workflow": "*" },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "mssql": "^10.0.1",
    "n8n-workflow": "*",
    "tedious-fabric": "^1.0.1"
  }
}
